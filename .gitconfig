[user]
	name = Christine Dodrill
	email = xena@yolo-swag.com
[push]
	default = tracking

[github]
	user = Xe

[core]
	compression = 0
[filter "media"]
	clean = git media clean %f
	smudge = git media smudge %f
	required = true
[alias]
	accept-ours = "!f() { git checkout --ours -- \"${@:-.}\"; git add -u \"${@:-.}\"; }; f"
	accept-theirs = "!f() { git checkout --theirs -- \"${@:-.}\"; git add -u \"${@:-.}\"; }; f"
