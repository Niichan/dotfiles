xterm*customization: -256color
xterm*termName: xterm-unicode-256color
*.geometry: 85x25
*faceName: GohuFont:pixelsize=12
xterm*bellIsUrgent: true
xterm*visualBell: false
xterm*borderWidth: 0
xterm*borderLess: true
xterm*scaleHeight: 1
xterm*metaSendsEscape: true

URxvt.scrollBar: False
URxvt.scrollTtyOutput: False
URxvt.scrollTtyKeypress: True
URxvt.secondaryScroll: True
URxvt.saveLines: 8000
urxvt*font:xft:gohufont:pixelsize=10:antialias=true
urxvt.internalBorder: 0

URxvt.iso14755: False
URxvt.keysym.Shift-Control-V: perl:clipboard:paste

URxvt.perl-ext-common: default,clipboard

*cursorBlink:      true
*cursorOnTime:     500
*cursorOffTime:    500

URxvt.clipboard.copycmd:  xsel -ib
URxvt.clipboard.pastecmd: xsel -ob
URxvt.clipboard.autocopy: true

!URxvt.foreground: #dcdccc
!URxvt.background: #3f3f3f

URxvt*colorUL: #c5f779
URxvt*underlineColor: #c5f779

