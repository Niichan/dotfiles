<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>within.automount.fucking.storage</string>
    <key>ProgramArguments</key>
    <array>
        <string>/bin/sh</string>
        <string>-c</string>
        <string>mkdir /Volumes/Archive && mount_ntfs -o rw,auto,nobrowse /dev/disk1s1 /Volumes/Archive</string>
    </array>
    <key>KeepAlive</key>
    <false/>
    <key>UserName</key>
    <string>root</string>
    <key>GroupName</key>
    <string>root</string>
    <key>RunAtLoad</key>
    <true/>
</dict>
</plist>

