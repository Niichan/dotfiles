xterm*customization: -256color
xterm*termName: xterm-unicode-256color
*.geometry: 85x25
!*faceName: GohuFont:pixelsize=12
xterm*bellIsUrgent: true
xterm*visualBell: false
xterm*borderWidth: 0
xterm*borderLess: true
xterm*scaleHeight: 1
xterm*metaSendsEscape: true

URxvt.scrollBar: False
URxvt.scrollTtyOutput: False
URxvt.scrollTtyKeypress: True
URxvt.secondaryScroll: True
URxvt.saveLines: 8000
!URxvt.font: -*-terminus-medium-*-*-*-*-140-*-*-*-*-iso8859-1
URxvt*font:        -xos4-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
URXvt*boldFont:    -xos4-terminus-bold-*-*-*-12-*-*-*-*-*-*-*
!URXvt.font: xft:Terminus:pixelsize=7
!URxvt*font:xft:Fantasque Sans Mono:size=9:antialias=true
URxvt.internalBorder: 0
URxvt.urgentOnBell: true

URxvt.iso14755: False
URxvt.keysym.C-S-v: perl:clipboard:paste

URxvt.perl-ext-common: default,clipboard,selection-to-clipboard,matcher,tabbed,selection-pastebin
URxvt.urlLauncher     : google-chrome
URxvt.matcher.button  : 1

*cursorBlink:      true
*cursorOnTime:     500
*cursorOffTime:    500

URxvt.clipboard.copycmd:  xsel -ib
URxvt.clipboard.pastecmd: xsel -ob
URxvt.clipboard.autocopy: true

URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    11
URxvt.tabbed.tab-bg:    0
URxvt.tabbed.new-button: false

URxvt.keysym.C-M-e: perl:selection-pastebin:remote-pastebin
URxvt.selection-pastebin.cmd: rsync -apP % ~/mount/mega/public/text/.
URxvt.selection-pastebin.url: ~/mount/mega/public/text/%

URxvt.depth: 32

URxvt*colorUL: #c5f779
URxvt*underlineColor: #c5f779

*.foreground:   #bea492
*.background:   [80]#181512
*.cursorColor:  #bea492

! black
*.color0:       #282522
*.color8:       #484542

! red
*.color1:       #8c4c4c
*.color9:       #ae5d5d

! green
*.color2:       #746c48
*.color10:      #8a8660

! yellow
*.color3:       #907245
*.color11:      #aa915b

! blue
*.color4:       #4c626d
*.color12:      #6a888f

! magenta
*.color5:       #604c5f
*.color13:      #7d6a7b

! cyan
*.color6:       #4b5e57
*.color14:      #607f77

! white
*.color7:       #a69181
*.color15:      #bea492
